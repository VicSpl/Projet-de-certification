{% extends 'base.html.twig' %}

{% block title %}Cat
{% endblock %}

{% block body %}
	<h1>Cat</h1>


	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ cat.id }}</td>
			</tr>
			<tr>
				<th>ImageFilename</th>
				<td>{{ cat.imageFilename }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ cat.name }}</td>
			</tr>
			<tr>
				<th>Dob</th>
				<td>{{ cat.dob ? cat.dob|date('Y-m-d') : '' }}</td>
			</tr>
			<tr>
				<th>City</th>
				<td>{{ cat.city }}</td>
			</tr>
			<tr>
				<th>Breed</th>
				<td>{{ cat.breed }}</td>
			</tr>
			<tr>
				<th>Gender</th>
				<td>{{ cat.gender }}</td>
			</tr>
			<tr>
				<th>Coat</th>
				<td>{{ cat.coat }}</td>
			</tr>
			<tr>
				<th>EyeColor</th>
				<td>{{ cat.eyeColor }}</td>
			</tr>
			<tr>
				<th>Weight</th>
				<td>{{ cat.weight }}</td>
			</tr>
			<tr>
				<th>Size</th>
				<td>{{ cat.size }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ cat.description }}</td>
			</tr>

		</tbody>
	</table>

	<a href="{{ path('profile_cat_index') }}">back to list</a>

	<a href="{{ path('profile_cat_edit', {'id': cat.id}) }}">edit</a>

	{{ include('profile_cat/_delete_form.html.twig') }}
{% endblock %}
