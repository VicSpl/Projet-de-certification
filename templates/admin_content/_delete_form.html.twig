<form method="post" action="{{ path('admin_content_delete', {'id': content.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet objet ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ content.id) }}">
	<button class="btn btn-brown m-3">Supprimer</button>
</form>
